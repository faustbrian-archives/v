{
	"name": "@veestore/veestore",
	"version": "0.1.0",
	"description": "A common interface for reusable Value Store implementations in TypeScript.",
	"homepage": "https://github.com/veestore/veestore",
	"bugs": {
		"url": "https://github.com/veestore/veestore/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:veestore/veestore.git"
	},
	"license": "MPL-2.0",
	"author": "Brian Faust <hello@basecode.sh>",
	"files": [
		"dist"
	],
	"main": "dist/index",
	"types": "dist/index",
	"scripts": {
		"build": "yarn clean && tsc",
		"build:watch": "yarn build -w",
		"clean": "rimraf .coverage dist tmp",
