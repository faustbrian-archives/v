		"@typeskrift/tsconfig": "^0.1.0",
		"codecov": "^3.5.0",
		"prettier": "^1.18.2",
		"rimraf": "^2.6.3",
		"tslint": "^5.17.0",
		"typescript": "^3.5.1"
	}
